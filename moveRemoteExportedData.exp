#!/usr/bin/expect
#moveRemoteExportedData.exp

set file [lindex $argv 0]
set target [lindex $argv 1]
set host [lindex $argv 2]
set passwd [lindex $argv 3]

set timeout 10

spawn ssh root@$host
expect {
  "*yes/no"     {send "yes\n"; exp_continue;}
  "*password:"  {send "$passwd\n";}
}

expect "#*"
send "\\cp -r $file $target\n"

send "exit\n"
expect eof



